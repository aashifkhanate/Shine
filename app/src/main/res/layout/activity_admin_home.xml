<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminZone.AdminHomeActivity">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <TextView
        android:id="@+id/tv_add_description"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="18dp"
        android:layout_weight="1"
        android:gravity="center"
        android:padding="8dp"
        android:text="Add Club Decription"
        android:textAlignment="center"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/admin_home_rounds_all"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="18dp"
        android:layout_weight="1"
        android:gravity="center"
        android:padding="8dp"
        android:text="Rounds"
        android:textAlignment="center"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/tv_add_coordinators"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="18dp"
        android:layout_weight="1"
        android:gravity="center"
        android:padding="8dp"
        android:text="Add/View Coordinators"
        android:textAlignment="center"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/tv_view_registered"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="18dp"
        android:layout_weight="1"
        android:gravity="center"
        android:padding="8dp"
        android:text="View Attendees"
        android:textAlignment="center"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/tv_result"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="18dp"
        android:layout_weight="1"
        android:gravity="center"
        android:padding="8dp"
        android:text="Result Section"
        android:textAlignment="center"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/tv_add_attendees"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="18dp"
        android:layout_weight="1"
        android:gravity="center"
        android:padding="8dp"
        android:text="Add Attendees"
        android:textAlignment="center"
        android:textSize="20dp" />
</LinearLayout>
</ScrollView>